'''
buyers = [["James", 1030],
          ["Curry", 893],
          ["Durant", 2050],
          ["Jordan", 990],
          ["David", 2210]]

goldbuyers = []
vipbuyers = []

while buyers:
    i = buyers.pop(0)
    if i[1] >= 1000:
        vipbuyers.append(i)
    else:
        goldbuyers.append(i)

print("VIP 買家資料", vipbuyers)
print("GOLD 買家資料", goldbuyers)
