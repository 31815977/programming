'''
num = int(input("輸入大於1的自然數 = "))
if(num == 2):
    print("%d是質數" % (num))
else:
    for i in range(2,int(num ** 0.5)+1):
        if(num % i == 0):
           print("%d不是質數" % (num))
           break
    else:
        print("%d是質數" % (num))
'''
'''
for i in range(1,10):
    for j in range(1,10):
        print("%d*%d = %-3d" % (i,j,i*j), end = " ")
    print()
