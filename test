def A(k):
    k=3000
    A=[]
    i=1
    for i in range(1,k+1):
        if i<=k:
           n1=2*i
           A.append(n1)
           i+1
    return A

def B(k):
    k=2000
    B=[]
    i=1
    for i in range(1,k+1):
        if i<=k:
           n1=3*i
           B.append(n1)
           i+1
    return B

def C(k):
    k=1200
    C=[]
    i=1
    for i in range(1,k+1):
        if i<=k:
           n1=5*i
           C.append(n1)
           i+1
    return C
x=set(A(3000))
y=set(B(2000))
z=set(C(1200))
print(len(x&y))
print(len(x&z))
print(len(y&z))
print(len(x&y&z))